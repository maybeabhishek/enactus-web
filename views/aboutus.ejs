    <%include frontpart/footer%>

    <script>

      if($(window).width() < 767)
      {
        $("#mobi").show();
        $("#pc").hide();
      } else {
        $("#pc").show();
        $("#mobi").hide();
      }
              
      $(document).ready(function() {
        $("#adv").toggleClass("active");
        $("#advMem").show();
        $("#finMem").hide();
        $("#mnsMem").hide();
        $("#creMem").hide();
        $("#hrMem").hide();
      });
      $("#adv").on("click", function() {
        $("#adv").addClass("active");
        $("#mns").removeClass("active");
        $("#fin").removeClass("active");
        $("#cre").removeClass("active");
        $("#hr").removeClass("active");
        $("#advMem").show();
        $("#finMem").hide();
        $("#mnsMem").hide();
        $("#creMem").hide();
        $("#hrMem").hide();
      });
      $("#fin").on("click", function() {
        $("#fin").addClass("active");
        $("#mns").removeClass("active");
        $("#adv").removeClass("active");
        $("#cre").removeClass("active");
        $("#hr").removeClass("active");
        $("#finMem").show();
        $("#advMem").hide();
        $("#mnsMem").hide();
        $("#creMem").hide();
        $("#hrMem").hide();
      });
      $("#mns").on("click", function() {
        $("#mns").addClass("active");
        $("#adv").removeClass("active");
        $("#fin").removeClass("active");
        $("#cre").removeClass("active");
        $("#hr").removeClass("active");
        $("#mnsMem").show();
        $("#finMem").hide();
        $("#advMem").hide();
        $("#creMem").hide();
        $("#hrMem").hide();
      });
      $("#hr").on("click", function() {
        $("#hr").addClass("active");
        $("#mns").removeClass("active");
        $("#fin").removeClass("active");
        $("#cre").removeClass("active");
        $("#adv").removeClass("active");
        $("#hrMem").show();
        $("#finMem").hide();
        $("#advMem").hide();
        $("#mnsMem").hide();
        $("#creMem").hide();
      });
      $("#cre").on("click", function() {
        $("#mns").removeClass("active");
        $("#cre").addClass("active");
        $("#fin").removeClass("active");
        $("#adv").removeClass("active");
        $("#hr").removeClass("active");
        $("#creMem").show();
        $("#finMem").hide();
        $("#advMem").hide();
        $("#mnsMem").hide();
        $("#hrMem").hide();
      });
    </script>
  </body>
</html>
