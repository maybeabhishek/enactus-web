    <div class="ui container mt-4" >
      <div class="ui top attached tabular menu stacked" id="pc">
        <a class="item" id="adv" >
          Advisory Board
        </a>
        <a class="item" id="fin">
          Finance
        </a>
        <a class="item" id="mns">
          Marketing and Sponsorship
        </a>
        <a class="item" id="hr">
          Human Resource
        </a>
        <a class="item" id="cre">
          Creative
        </a>
      </div>
      <!-- Advisor Board Members -->
      <div class="ui bottom attached segment" id="advMem">
        <div class="ui link cards centered">
                <% for(var i=0; i<advMembers.length; i++) {%>
            <div class="card" >
            <div class="image">
              <img src="/images/members/<%=advMembers[i].photo%>" />
            </div>
            <div class="content">
              <div class="header"><%=advMembers[i].name%></div>
              <div class="meta">
                <a><%=advMembers[i].desg%></a>
              </div>
              <div class="description">
                    <%=advMembers[i].desc%>
              </div>
            </div>
            <div class="extra content">
              <span class="right floated">
                Joined in 2013
              </span>
              <span>
                <i class="user icon"></i>
                75 Friends
              </span>
            </div>
          </div>
          <%}%>
        </div>
      </div>

